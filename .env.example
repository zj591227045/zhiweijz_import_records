# 记账数据导入工具 - 环境变量配置示例

# 应用基础配置
VITE_APP_TITLE=记账数据导入工具
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=专为"只为记账"系统设计的数据导入工具

# API 配置
# 默认API服务器地址（可选，用户可在应用中配置）
VITE_DEFAULT_API_BASE_URL=https://app.zhiweijz.cn:1443

# 官方服务器配置
VITE_OFFICIAL_SERVER_URL=https://app.zhiweijz.cn:1443
VITE_OFFICIAL_SERVER_NAME=官方服务器

# 应用功能配置
# 文件上传大小限制（MB）
VITE_MAX_FILE_SIZE=10
# 支持的文件类型
VITE_SUPPORTED_FILE_TYPES=.xlsx,.xls,.csv
# 批量导入每批次大小
VITE_BATCH_SIZE=100

# 开发环境配置
# 是否启用调试模式
VITE_DEBUG_MODE=false
# 是否显示详细日志
VITE_VERBOSE_LOGGING=false

# Vercel 部署配置
# 构建环境
NODE_ENV=production
# Vercel 区域设置
VERCEL_REGION=hkg1

# 安全配置
# 是否启用 HTTPS 重定向
VITE_FORCE_HTTPS=true
# CSP 配置
VITE_CSP_ENABLED=true

# 分析和监控（可选）
# 是否启用性能分析
VITE_ANALYTICS_ENABLED=false
# Google Analytics ID（如果需要）
VITE_GA_ID=

# 错误报告（可选）
# Sentry DSN（如果需要）
VITE_SENTRY_DSN=
